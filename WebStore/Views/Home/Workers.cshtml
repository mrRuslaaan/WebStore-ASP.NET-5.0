@model IEnumerable<WebStore.Models.Worker>

<!DOCTYPE html>
<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Workers</title>
</head>
<body>
    <table>
        <thead>
            <tr>
                <th>ID</th>
                <th>Фамилия</th>
                <th>Имя</th>
                <th>Отчество</th>
                <th>Возраст</th>
            </tr>
        </thead>
        <tbody>
            @foreach (var worker in Model)
            {
                <tr>
                    <td>@worker.Id</td> 
                    <td>@worker.LastName</td>
                    <td>@worker.FirstName</td>
                    <td>@worker.Patronymic</td>
                    <td>@worker.Age</td>
                </tr>
            }
        </tbody>
    </table>
    <a href="~/">Домой</a>
</body>
</html>
