@model Worker



<div class="container">
    <table class="table">
        <thead>
            <tr>
                <th>ID</th>
                <th>Фамилия</th>
                <th>Имя</th>
                <th>Отчество</th>
                <th>Возраст</th>
                <th>Должность</th>
            </tr>
        </thead>
        <tbody>

            <tr>
                <td>@Model.Id</td>
                <td>@Model.LastName</td>
                <td>@Model.FirstName</td>
                <td>@Model.Patronymic</td>
                <td>@Model.Age</td>
                <td>@Model.Position</td>
            </tr>

        </tbody>
    </table>
    <a asp-controller="Home" asp-action="Index">Домой</a>
    <a asp-controller="Worker" asp-action="Index">Назад</a>
</div>